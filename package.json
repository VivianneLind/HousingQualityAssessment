{
  "name": "privacy-housing-assessment",
  "version": "3.0.0",
  "description": "Privacy-preserving housing quality assessment system using FHEVM",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:analyze": "npm run build && open dist/stats.html",
    "preview": "vite preview",
    "compile": "set TS_NODE_PROJECT=tsconfig.hardhat.json && hardhat compile",
    "typechain": "hardhat typechain",
    "deploy:sepolia": "set TS_NODE_PROJECT=tsconfig.hardhat.json&& hardhat run scripts/deploy-simple.cjs --network sepolia",
    "verify": "set TS_NODE_PROJECT=tsconfig.hardhat.json&& hardhat run scripts/verify.cjs --network sepolia",
    "interact": "set TS_NODE_PROJECT=tsconfig.hardhat.json&& hardhat run scripts/interact.cjs --network sepolia",
    "simulate": "set TS_NODE_PROJECT=tsconfig.hardhat.json&& hardhat run scripts/simulate.cjs --network sepolia",
    "verify-assessment": "set TS_NODE_PROJECT=tsconfig.hardhat.json&& hardhat run scripts/verify-assessment.cjs --network sepolia",
    "test": "npm run test:mock && npm run test:sepolia",
    "test:mock": "hardhat test test/HousingAssessment.ts",
    "test:sepolia": "hardhat test test/HousingAssessmentSepolia.ts --network sepolia",
    "test:watch": "hardhat watch test",
    "test:gas": "REPORT_GAS=true npm run test:mock",
    "coverage": "hardhat coverage",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:security": "eslint . --ext ts,tsx --config .eslintrc.security.json",
    "lint:contracts": "solhint 'contracts/**/*.sol'",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:contracts": "prettier --write \"contracts/**/*.sol\"",
    "typecheck": "tsc --noEmit",
    "security:audit": "npm audit --audit-level=moderate",
    "security:check": "npm run lint:security && npm run security:audit",
    "size": "npm run build && du -sh dist/ && find dist -name '*.js' -exec du -h {} \\;",
    "prepare": "husky install",
    "precommit": "npm run lint && npm run typecheck && npm run test:mock",
    "prepush": "npm run test:mock && npm run build"
  },
  "keywords": [
    "blockchain",
    "fhevm",
    "privacy",
    "housing",
    "assessment",
    "zama",
    "web3",
    "ethereum"
  ],
  "author": "Privacy Assessment Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@fhevm/solidity": "^0.8.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@rainbow-me/rainbowkit": "^2.1.0",
    "@tanstack/react-query": "^5.28.0",
    "@wagmi/core": "^2.8.0",
    "@zama-fhe/oracle-solidity": "^0.2.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "dotenv": "^16.4.5",
    "ethers": "^6.11.1",
    "lucide-react": "^0.356.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.2",
    "viem": "^2.9.0",
    "wagmi": "^2.5.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.6",
    "@nomicfoundation/hardhat-ethers": "^3.0.5",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.14",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "chai": "^4.4.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-security": "^2.1.0",
    "fhevm": "^0.5.0",
    "hardhat": "^2.22.0",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-deploy": "^0.12.4",
    "hardhat-gas-reporter": "^1.0.10",
    "husky": "^9.0.11",
    "mocha": "^10.4.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-solidity": "^1.3.1",
    "solhint": "^4.5.2",
    "solhint-plugin-prettier": "^0.1.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "solidity-coverage": "^0.8.11",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "terser": "^5.28.1",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "^5.4.3",
    "vite": "^5.2.0"
  }
}
